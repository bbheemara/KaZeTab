{
  "name": "KaZeTab",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Changes new tab with type of wallpaper you want!",
  "permissions": [],

  "background": {
    "service_worker": "background.js"
  },

  "action": {
    "default_icon": {},
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      
      "css": ["./css/styles.css"]
    }
  ]
}
